<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension point="org.eclipse.ui.commands">
   	<command
          categoryId="org.eclipse.ui.category.navigate"
          defaultHandler="pp.eclipse.command.OpenInputTemplateCommand"
          description="Browse Input Templates and select one to open"
          id="pp.eclipse.command.OpenInputTemplateCommand"
          name="Open Input Template"> 	
	</command>
   </extension>
   
   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="pp.eclipse.command.OpenInputTemplateCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+Shift+I">
	  </key>
   </extension>

</plugin>
